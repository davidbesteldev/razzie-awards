model Movie {
  id     Int     @id @default(autoincrement())
  year   Int
  title  String
  winner Boolean @default(false)

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  producers MovieProducer[]
  studios   MovieStudio[]

  @@map("movies")
}
